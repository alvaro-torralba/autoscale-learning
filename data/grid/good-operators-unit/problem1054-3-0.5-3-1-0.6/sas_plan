(pickup pos3-0 key1)
(unlock pos3-0 pos2-0 key1 shape1)
(move pos3-0 pos2-0)
(move pos2-0 pos1-0)
(pickup-and-loose pos1-0 key0 key1)
(unlock pos1-0 pos1-1 key0 shape0)
(move pos1-0 pos1-1)
(move pos1-1 pos1-2)
(move pos1-2 pos1-3)
(unlock pos1-3 pos0-3 key0 shape0)
(move pos1-3 pos0-3)
(putdown pos0-3 key0)
