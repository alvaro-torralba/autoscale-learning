(board-truck driver3 truck2 s3)
(drive-truck truck2 s3 s0 driver3)
(disembark-truck driver3 truck2 s0)
(walk driver3 s0 p2-0)
(walk driver3 p2-0 s2)
(walk driver2 s1 p1-2)
(walk driver2 p1-2 s2)
(load-truck package1 truck4 s2)
(board-truck driver3 truck4 s2)
(drive-truck truck4 s2 s3 driver3)
(unload-truck package1 truck4 s3)
(load-truck package3 truck4 s3)
(board-truck driver2 truck3 s2)
(drive-truck truck3 s2 s1 driver2)
(drive-truck truck4 s3 s2 driver3)
(unload-truck package3 truck4 s2)
(drive-truck truck4 s2 s1 driver3)
(disembark-truck driver3 truck4 s1)
; cost = 18 (unit cost)
