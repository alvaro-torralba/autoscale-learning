(board-truck driver3 truck4 s2)
(drive-truck truck4 s2 s1 driver3)
(drive-truck truck4 s1 s0 driver3)
(load-truck package1 truck4 s0)
(disembark-truck driver3 truck4 s0)
(board-truck driver6 truck4 s0)
(drive-truck truck4 s0 s3 driver6)
(unload-truck package1 truck4 s3)
(board-truck driver4 truck6 s3)
(drive-truck truck6 s3 s0 driver4)
(disembark-truck driver4 truck6 s0)
(walk driver4 s0 p0-1)
