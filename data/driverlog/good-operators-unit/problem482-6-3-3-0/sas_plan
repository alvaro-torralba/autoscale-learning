(walk driver1 s1 p2-1)
(walk driver1 p2-1 s2)
(board-truck driver4 truck3 s0)
(drive-truck truck3 s0 s2 driver4)
(load-truck package1 truck3 s2)
(drive-truck truck3 s2 s1 driver4)
(disembark-truck driver4 truck3 s1)
(unload-truck package1 truck3 s1)
(walk driver5 s1 p2-1)
(walk driver5 p2-1 s2)
; cost = 10 (unit cost)
