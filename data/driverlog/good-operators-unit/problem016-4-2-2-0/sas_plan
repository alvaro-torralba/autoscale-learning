(load-truck package2 truck3 s0)
(board-truck driver2 truck4 s1)
(drive-truck truck4 s1 s0 driver2)
(disembark-truck driver2 truck4 s0)
(board-truck driver4 truck1 s1)
(board-truck driver2 truck3 s0)
(drive-truck truck1 s1 s0 driver4)
(disembark-truck driver4 truck1 s0)
(drive-truck truck3 s0 s1 driver2)
(unload-truck package2 truck3 s1)
; cost = 10 (unit cost)
