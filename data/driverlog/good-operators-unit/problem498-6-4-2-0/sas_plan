(walk driver2 s0 p0-1)
(walk driver2 p0-1 s1)
(board-truck driver5 truck2 s1)
(drive-truck truck2 s1 s0 driver5)
(load-truck package4 truck2 s0)
(disembark-truck driver5 truck2 s0)
(board-truck driver3 truck5 s1)
(board-truck driver1 truck2 s0)
(drive-truck truck2 s0 s1 driver1)
(disembark-truck driver1 truck2 s1)
(drive-truck truck5 s1 s0 driver3)
(disembark-truck driver3 truck5 s0)
(unload-truck package4 truck2 s1)
; cost = 13 (unit cost)
