(walk driver3 s0 p0-1)
(walk driver3 p0-1 s1)
(load-truck package2 truck3 s1)
(load-truck package1 truck4 s0)
(board-truck driver6 truck3 s1)
(drive-truck truck3 s1 s0 driver6)
(board-truck driver1 truck4 s0)
(drive-truck truck4 s0 s1 driver1)
(disembark-truck driver1 truck4 s1)
(unload-truck package1 truck4 s1)
(unload-truck package2 truck3 s0)
; cost = 11 (unit cost)
