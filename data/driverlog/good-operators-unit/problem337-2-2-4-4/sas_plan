(load-truck package1 truck6 s1)
(board-truck driver2 truck2 s2)
(drive-truck truck2 s2 s1 driver2)
(disembark-truck driver2 truck2 s1)
(board-truck driver2 truck6 s1)
(drive-truck truck6 s1 s3 driver2)
(unload-truck package1 truck6 s3)
(drive-truck truck6 s3 s0 driver2)
(disembark-truck driver2 truck6 s0)
(walk driver2 s0 p1-0)
(board-truck driver1 truck4 s2)
(drive-truck truck4 s2 s0 driver1)
(disembark-truck driver1 truck4 s0)
(board-truck driver1 truck5 s0)
(drive-truck truck5 s0 s2 driver1)
(disembark-truck driver1 truck5 s2)
(walk driver1 s2 p2-3)
(walk driver1 p2-3 s3)
(walk driver2 p1-0 s1)
; cost = 19 (unit cost)
