(walk driver1 s3 p3-2)
(walk driver1 p3-2 s2)
(board-truck driver1 truck4 s2)
(drive-truck truck4 s2 s1 driver1)
(disembark-truck driver1 truck4 s1)
(board-truck driver1 truck3 s1)
(drive-truck truck3 s1 s2 driver1)
(disembark-truck driver1 truck3 s2)
(board-truck driver2 truck5 s2)
(board-truck driver1 truck2 s2)
(drive-truck truck2 s2 s3 driver1)
(load-truck package1 truck5 s2)
(load-truck package2 truck5 s2)
(drive-truck truck5 s2 s0 driver2)
(unload-truck package1 truck5 s0)
(unload-truck package2 truck5 s0)
; cost = 16 (unit cost)
