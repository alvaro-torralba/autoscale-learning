(walk driver5 s0 p0-2)
(walk driver5 p0-2 s2)
(board-truck driver3 truck5 s2)
(drive-truck truck5 s2 s0 driver3)
(disembark-truck driver3 truck5 s0)
(walk driver3 s0 p0-1)
(walk driver3 p0-1 s1)
(load-truck package3 truck4 s3)
(load-truck package2 truck4 s3)
(board-truck driver5 truck3 s2)
(board-truck driver2 truck1 s0)
(board-truck driver1 truck4 s3)
(drive-truck truck1 s0 s3 driver2)
(disembark-truck driver2 truck1 s3)
(drive-truck truck3 s2 s3 driver5)
(disembark-truck driver5 truck3 s3)
(drive-truck truck4 s3 s2 driver1)
(unload-truck package3 truck4 s2)
(drive-truck truck4 s2 s0 driver1)
(unload-truck package2 truck4 s0)
; cost = 20 (unit cost)
