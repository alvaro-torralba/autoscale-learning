(walk driver4 s0 p0-2)
(walk driver4 p0-2 s2)
(walk driver3 s0 p1-0)
(walk driver3 p1-0 s1)
(walk driver1 s3 p3-2)
(walk driver1 p3-2 s2)
(load-truck package1 truck4 s1)
(board-truck driver3 truck4 s1)
(board-truck driver1 truck5 s2)
(drive-truck truck4 s1 s3 driver3)
(drive-truck truck5 s2 s1 driver1)
(drive-truck truck5 s1 s0 driver1)
(disembark-truck driver1 truck5 s0)
(unload-truck package1 truck4 s3)
; cost = 14 (unit cost)
