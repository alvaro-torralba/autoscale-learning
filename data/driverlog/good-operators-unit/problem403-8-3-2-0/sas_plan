(board-truck driver8 truck3 s1)
(drive-truck truck3 s1 s0 driver8)
(unload-truck package2 truck3 s0)
(disembark-truck driver8 truck3 s0)
(board-truck driver6 truck6 s0)
(board-truck driver5 truck1 s0)
(drive-truck truck1 s0 s1 driver5)
(drive-truck truck6 s0 s1 driver6)
(disembark-truck driver6 truck6 s1)
