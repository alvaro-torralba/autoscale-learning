(load-truck package2 truck2 s1)
(board-truck driver2 truck2 s1)
(drive-truck truck2 s1 s2 driver2)
(disembark-truck driver2 truck2 s2)
(board-truck driver3 truck2 s2)
(unload-truck package2 truck2 s2)
