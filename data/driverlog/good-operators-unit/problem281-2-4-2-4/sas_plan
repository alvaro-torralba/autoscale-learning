(walk driver2 s0 p0-1)
(walk driver2 p0-1 s1)
(board-truck driver1 truck4 s0)
(drive-truck truck4 s0 s1 driver1)
(disembark-truck driver1 truck4 s1)
(board-truck driver2 truck5 s1)
(board-truck driver1 truck2 s1)
