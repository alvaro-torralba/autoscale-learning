(load-truck package1 truck4 s0)
(board-truck driver3 truck4 s0)
(drive-truck truck4 s0 s1 driver3)
(unload-truck package1 truck4 s1)
(drive-truck truck4 s1 s2 driver3)
(disembark-truck driver3 truck4 s2)
(walk driver3 s2 p2-1)
(board-truck driver4 truck3 s1)
(board-truck driver2 truck1 s1)
(drive-truck truck1 s1 s0 driver2)
(drive-truck truck3 s1 s2 driver4)
(disembark-truck driver4 truck3 s2)
(walk driver3 p2-1 s1)
; cost = 13 (unit cost)
