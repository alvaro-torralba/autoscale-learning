(board-truck driver1 truck1 s2)
(drive-truck truck1 s2 s3 driver1)
(load-truck package1 truck1 s3)
(drive-truck truck1 s3 s2 driver1)
(drive-truck truck1 s2 s5 driver1)
(unload-truck package1 truck1 s5)
(drive-truck truck1 s5 s6 driver1)
(disembark-truck driver1 truck1 s6)
(board-truck driver1 truck2 s6)
(drive-truck truck2 s6 s4 driver1)
(disembark-truck driver1 truck2 s4)
(walk driver1 s4 p4-3)
(walk driver1 p4-3 s3)
; cost = 13 (unit cost)
