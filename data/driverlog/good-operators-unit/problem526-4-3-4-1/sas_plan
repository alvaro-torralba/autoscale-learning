(walk driver3 s1 p3-1)
(walk driver2 s2 p1-2)
(walk driver2 p1-2 s1)
(load-truck package3 truck4 s1)
(load-truck package1 truck1 s2)
(board-truck driver4 truck4 s1)
(drive-truck truck4 s1 s2 driver4)
(disembark-truck driver4 truck4 s2)
(board-truck driver4 truck1 s2)
(board-truck driver1 truck2 s0)
(drive-truck truck1 s2 s3 driver4)
(drive-truck truck2 s0 s3 driver1)
(disembark-truck driver1 truck2 s3)
(unload-truck package1 truck1 s3)
(drive-truck truck1 s3 s0 driver4)
(unload-truck package3 truck4 s2)
(walk driver3 p3-1 s3)
; cost = 17 (unit cost)
