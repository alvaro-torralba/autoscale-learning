(load-truck package1 truck5 s1)
(board-truck driver2 truck5 s1)
(drive-truck truck5 s1 s0 driver2)
(unload-truck package1 truck5 s0)
(load-truck package2 truck4 s0)
(disembark-truck driver2 truck5 s0)
(board-truck driver2 truck4 s0)
(board-truck driver1 truck2 s1)
(drive-truck truck2 s1 s0 driver1)
(disembark-truck driver1 truck2 s0)
(drive-truck truck4 s0 s1 driver2)
(disembark-truck driver2 truck4 s1)
(unload-truck package2 truck4 s1)
(walk driver1 s0 p2-0)
(walk driver1 p2-0 s2)
; cost = 15 (unit cost)
