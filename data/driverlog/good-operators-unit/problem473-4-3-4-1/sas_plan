(walk driver1 s1 p0-1)
(load-truck package1 truck3 s3)
(board-truck driver3 truck3 s3)
(drive-truck truck3 s3 s0 driver3)
(drive-truck truck3 s0 s2 driver3)
(unload-truck package1 truck3 s2)
(drive-truck truck3 s2 s1 driver3)
(disembark-truck driver3 truck3 s1)
(walk driver1 p0-1 s0)
(walk driver3 s1 p1-3)
(walk driver3 p1-3 s3)
(walk driver4 s2 p0-2)
(walk driver4 p0-2 s0)
; cost = 13 (unit cost)
