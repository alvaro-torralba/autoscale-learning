(walk driver3 s1 p3-1)
(board-truck driver4 truck2 s0)
(drive-truck truck2 s0 s1 driver4)
(load-truck package2 truck2 s1)
(board-truck driver2 truck4 s0)
(drive-truck truck4 s0 s3 driver2)
(drive-truck truck2 s1 s2 driver4)
(disembark-truck driver2 truck4 s3)
(board-truck driver2 truck1 s3)
(drive-truck truck1 s3 s1 driver2)
(unload-truck package2 truck2 s2)
(drive-truck truck2 s2 s1 driver4)
(walk driver3 p3-1 s3)
; cost = 13 (unit cost)
