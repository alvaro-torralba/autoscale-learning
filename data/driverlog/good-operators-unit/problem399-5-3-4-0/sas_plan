(load-truck package3 truck1 s1)
(board-truck driver5 truck1 s1)
(drive-truck truck1 s1 s0 driver5)
(load-truck package2 truck1 s0)
(drive-truck truck1 s0 s3 driver5)
(unload-truck package2 truck1 s3)
(disembark-truck driver5 truck1 s3)
(board-truck driver5 truck4 s3)
(board-truck driver1 truck5 s2)
(drive-truck truck4 s3 s0 driver5)
(disembark-truck driver5 truck4 s0)
(drive-truck truck5 s2 s0 driver1)
(unload-truck package3 truck1 s3)
; cost = 13 (unit cost)
