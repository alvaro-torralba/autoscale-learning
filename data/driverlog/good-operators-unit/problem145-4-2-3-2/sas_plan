(load-truck package1 truck4 s1)
(board-truck driver1 truck6 s1)
(drive-truck truck6 s1 s2 driver1)
(disembark-truck driver1 truck6 s2)
(board-truck driver1 truck5 s2)
(drive-truck truck5 s2 s1 driver1)
(disembark-truck driver1 truck5 s1)
(board-truck driver1 truck4 s1)
(drive-truck truck4 s1 s0 driver1)
(disembark-truck driver1 truck4 s0)
(unload-truck package1 truck4 s0)
(walk driver1 s0 p0-1)
(walk driver1 p0-1 s1)
; cost = 13 (unit cost)
