(walk driver1 s7 p7-4)
(walk driver1 p7-4 s4)
(walk driver1 s4 p2-4)
(walk driver1 p2-4 s2)
(board-truck driver1 truck1 s2)
(drive-truck truck1 s2 s1 driver1)
(load-truck package2 truck1 s1)
(drive-truck truck1 s1 s0 driver1)
(drive-truck truck1 s0 s7 driver1)
(load-truck package1 truck1 s7)
(drive-truck truck1 s7 s6 driver1)
(unload-truck package1 truck1 s6)
(drive-truck truck1 s6 s4 driver1)
(unload-truck package2 truck1 s4)
(drive-truck truck1 s4 s0 driver1)
; cost = 15 (unit cost)
