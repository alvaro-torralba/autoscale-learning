(walk driver4 s0 p0-1)
(walk driver4 p0-1 s1)
(load-truck package1 truck4 s1)
(board-truck driver4 truck4 s1)
(drive-truck truck4 s1 s0 driver4)
(board-truck driver1 truck8 s0)
(drive-truck truck8 s0 s1 driver1)
(disembark-truck driver1 truck8 s1)
(unload-truck package1 truck4 s0)
; cost = 9 (unit cost)
