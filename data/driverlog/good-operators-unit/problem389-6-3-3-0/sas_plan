(walk driver6 s1 p2-1)
(walk driver6 p2-1 s2)
(load-truck package1 truck4 s1)
(board-truck driver4 truck4 s1)
(drive-truck truck4 s1 s0 driver4)
(load-truck package3 truck4 s0)
(drive-truck truck4 s0 s2 driver4)
(disembark-truck driver4 truck4 s2)
(board-truck driver1 truck3 s2)
(drive-truck truck3 s2 s0 driver1)
(disembark-truck driver1 truck3 s0)
(unload-truck package1 truck4 s2)
(unload-truck package3 truck4 s2)
; cost = 13 (unit cost)
