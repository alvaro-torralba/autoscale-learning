(walk driver3 s1 p2-1)
(walk driver3 p2-1 s2)
(board-truck driver4 truck2 s2)
(drive-truck truck2 s2 s0 driver4)
(load-truck package2 truck2 s0)
(drive-truck truck2 s0 s3 driver4)
(unload-truck package2 truck2 s3)
(drive-truck truck2 s3 s0 driver4)
(disembark-truck driver4 truck2 s0)
(walk driver5 s1 p1-3)
(walk driver5 p1-3 s3)
; cost = 11 (unit cost)
