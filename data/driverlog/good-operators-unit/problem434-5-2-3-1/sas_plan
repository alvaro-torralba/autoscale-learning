(load-truck package1 truck1 s2)
(board-truck driver5 truck6 s2)
(drive-truck truck6 s2 s0 driver5)
(board-truck driver4 truck3 s1)
(drive-truck truck3 s1 s0 driver4)
(disembark-truck driver5 truck6 s0)
(disembark-truck driver4 truck3 s0)
(board-truck driver2 truck4 s1)
(board-truck driver1 truck1 s2)
(drive-truck truck1 s2 s1 driver1)
(disembark-truck driver1 truck1 s1)
(drive-truck truck4 s1 s2 driver2)
(unload-truck package1 truck1 s1)
; cost = 13 (unit cost)
