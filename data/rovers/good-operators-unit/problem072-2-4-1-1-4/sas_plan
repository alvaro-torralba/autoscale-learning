(sample_rock rover1 rover1store waypoint2)
(navigate rover1 waypoint2 waypoint0)
(calibrate rover1 camera0 objective2 waypoint0)
(communicate_rock_data rover1 general waypoint2 waypoint0 waypoint3)
(drop rover1 rover1store)
(sample_soil rover1 rover1store waypoint0)
(communicate_soil_data rover1 general waypoint0 waypoint0 waypoint3)
(take_image rover1 waypoint0 objective3 camera0 colour)
(communicate_image_data rover1 general objective3 colour waypoint0 waypoint3)
